<%= render 'shared/page_title', title: "Log in" %>

  <div class="row">
    <div class="well col-md-6 col-md-offset-3">
      <%= bootstrap_form_tag url: '/login' do |f| %>
        
        <%= f.text_field :email %>
        
        <%= f.password_field :password %>
        
        <%= f.submit "Login", class: "btn btn-success" %>
        
      <% end %>
    </div>
  </div>